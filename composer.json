{
  "name": "t1k3/laravel-translatable",
  "description": "Laravel Translatable",
  "keywords": [
    "laravel-translatable",
    "laravel",
    "translatable",
    "translate"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7",
    "orchestra/testbench": "^3.4",
    "phpmd/phpmd": "^2.6"
  },
  "autoload": {
    "classmap": [
      "src/database"
    ],
    "psr-4": {
      "T1k3\\LaravelTranslatable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "T1k3\\LaravelTranslatable\\Tests\\": "tests",
      "T1k3\\LaravelTranslatable\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "T1k3\\LaravelTranslatable\\Providers\\ServiceProvider"
      ]
    }
  }
}